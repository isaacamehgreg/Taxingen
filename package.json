  "scripts": {
    "build": "tsc && cp -r ./src/Emails/ ./dist/src/Emails && echo file is copied",
    "lint": "eslint ./src/**/*.ts",
    "format": "eslint ./src/**/*.ts --fix",
    "start": "node dist/src/index.js",
    "dev": "nodemon --watch './**/*.ts' --exec 'ts-node' ./src/index.ts ",
    "test": "jest -i --verbose",
    "pretypeorm": "ts-node -r tsconfig-paths/register ./scripts/write-type-orm-config.ts",
    "migration:generate": "yarn run typeorm -- migration:generate -n",
    "migration:run": "npm run typeorm -- migration:run",
    "migrate:dev": "yarn typeorm && yarn migration:generate && yarn migration:run",
    "create:migration": "ts-node ./node_modules/typeorm/cli.js migration:generate -n PostRefactoring -p",
    "run:migration": "typeorm migration:run"
  },
